      "link": true
    },
    "custom": {
      "spacing": {
        "unit": "15px"
      },
      "typography": {
        "letter-spacing": {
          "body": "-0.01em",
          "heading": "-0.02em"
        },
        "line-height": {
          "body": "1.7",
          "heading": "1.25"
        }
      }
    },
    "layout": {
      "contentSize": "780px",
      "wideSize": "1366px"
    },
    "spacing": {
      "blockGap": true,
      "spacingSizes": [
        {
          "size": "20px",
          "slug": "x-small",
          "name": "xSmall"
        },
        {
          "size": "40px",
          "slug": "small",
          "name": "Small"
        },
        {
          "size": "60px",
          "slug": "medium",
          "name": "Medium"
        },
        {
          "size": "80px",
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "100px",
          "slug": "X-large",
          "name": "xLarge"
        }
      ],
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "customFontSize": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Zen Maru Gothic\", ui-sans-serif, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
          "slug": "default",
          "name": "BIZ UDPGothic",
          "fontFace": [
            {
              "fontFamily": "BIZ UDPGothic",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [ "file:./assets/fonts/BIZUDPGothic-Regular.ttf"]
            },
            {
              "fontFamily": "BIZ UDPGothic",
              "fontWeight": "700",
              "fontStyle": "Bold",
              "fontStretch": "normal",
              "src": [ "file:./assets/fonts/BIZUDPGothic-Bold.ttf"]
