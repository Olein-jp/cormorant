    "custom": {
      "spacing": {
        "unit": "1rem"
      },
      "typography": {
        "harmonic-sequence-base": {
          "min": "8",
          "standard": "9",
          "max": "10"
        },
        "size-level": {
          "xs": "-2",
          "s": "-1",
          "m": "0",
          "l": "1",
          "xl": "2",
          "2-xl": "3",
          "3-xl": "4",
          "4-xl": "5",
          "5-xl": "6",
          "6:xl": "7"
        },
        "calculated": {
          "font-size": {
            "xs": {
              "min": "calc(1rem * var(--wp--custom--typography--harmonic-sequence-base--min) / (var(--wp--custom--typography--harmonic-sequence-base--min) - var(--wp--custom--typography--size-level--xs)) )",
              "standard": "calc(1rem * var(--wp--custom--typography--harmonic-sequence-base--standard) / (var(--wp--custom--typography--harmonic-sequence-base--standard) - var(--wp--custom--typography--size-level--xs)) )",
              "max": "calc(1rem * var(--wp--custom--typography--harmonic-sequence-base--max) / (var(--wp--custom--typography--harmonic-sequence-base--max) - var(--wp--custom--typography--size-level--xs)) )"
            }
          }
        },
        "letter-spacing": {
          "body": "0em",
          "heading": "0em"
        },
        "line-height": {
          "tiny": 1.1,
          "small": 1.2,
          "medium": 1.4,
          "normal": 1.7
        }
      }
    },
