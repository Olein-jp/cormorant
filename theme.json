        "heading": 1.5
      },
      "default-style": {
        "border": {
          "width": "1px",
          "style": "solid",
          "color": "currentColor"
        }
      },
      "spacing": {
        "base": "15px"
      }
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1100px"
    },
    "spacing": {
      "blockGap": null,
      "customPadding": true,
      "customMargin": true,
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
